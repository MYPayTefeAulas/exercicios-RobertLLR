<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".DadosActivity">

    <Button
        android:id="@+id/btTrocarDados1"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="80dp"
        android:layout_marginEnd="48dp"
        android:text=">"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btTrocarDados2"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="24dp"
        android:text=">"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/btTrocarDados1"
        app:layout_constraintStart_toStartOf="@+id/btTrocarDados1"
        app:layout_constraintTop_toBottomOf="@+id/btTrocarDados1" />

    <Button
        android:id="@+id/btTrocarDados3"
        android:layout_width="50dp"
        android:layout_height="50dp"
        android:layout_marginTop="25dp"
        android:text=">"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/btTrocarDados1"
        app:layout_constraintStart_toStartOf="@+id/btTrocarDados1"
        app:layout_constraintTop_toBottomOf="@+id/btTrocarDados2" />

    <Button
        android:id="@+id/btRolarDados"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="220dp"
        android:text="Rolar os Dados"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btTrocarDados1" />

    <TextView
        android:id="@+id/txtSaidaDados1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="20dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/btTrocarDados1"
        app:layout_constraintEnd_toStartOf="@+id/txtDados1"
        app:layout_constraintTop_toTopOf="@+id/btTrocarDados1" />

    <TextView
        android:id="@+id/txtSaidaDados2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/txtSaidaDados1"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txtSaidaDados1"
        app:layout_constraintTop_toBottomOf="@+id/txtSaidaDados1" />

    <TextView
        android:id="@+id/txtSaidaDados3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="TextView"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/txtSaidaDados2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txtSaidaDados2"
        app:layout_constraintTop_toBottomOf="@+id/txtSaidaDados2" />

    <TextView
        android:id="@+id/txtDados1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="52dp"
        android:text="6 lados"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="@+id/btTrocarDados1"
        app:layout_constraintEnd_toStartOf="@+id/btTrocarDados1"
        app:layout_constraintTop_toTopOf="@+id/btTrocarDados1" />

    <TextView
        android:id="@+id/txtDados2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        android:text="10 lados"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/txtDados1"
        app:layout_constraintHorizontal_bias="0.545"
        app:layout_constraintStart_toStartOf="@+id/txtDados1"
        app:layout_constraintTop_toBottomOf="@+id/txtDados1" />

    <TextView
        android:id="@+id/txtDados3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="52dp"
        android:text="20 lados"
        android:textSize="20sp"
        app:layout_constraintEnd_toEndOf="@+id/txtDados2"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="@+id/txtDados2"
        app:layout_constraintTop_toBottomOf="@+id/txtDados2" />

    <TextView
        android:id="@+id/textView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ROLAGEM DE 3 DADOS"
        android:textSize="34sp"
        tools:layout_editor_absoluteX="3dp"
        tools:layout_editor_absoluteY="6dp" />

</androidx.constraintlayout.widget.ConstraintLayout>